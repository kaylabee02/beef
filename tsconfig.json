{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "dom",
      "es2015"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "moduleResolution": "node",
    "jsx": "react-native",
    "noEmit": true,
    "isolatedModules": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
, "API/ProductServicetsx"  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
